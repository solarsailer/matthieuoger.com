  "description": "Solar Sailer by Matthieu Oger",
  "license": "UNLICENSED",
  "private": true,
  "repository": "solarsailer/solarsailer.github.io",
  "author": {
    "name": "Matthieu Oger",
    "url": "solarsailer.net"
  },
  "scripts": {
    "start": "bundle exec jekyll build --watch & webpack --watch",
    "build": "yarn run build:webpack && yarn run build:jekyll",
    "build:webpack": "webpack -p --env.production",
    "build:jekyll": "bundle exec jekyll clean && JEKYLL_ENV=production bundle exec jekyll build",
    "test": "standard | snazzy"
  },
  "dependencies": {
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "sass-loader": "^6.0.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "webpack": "^2.6.1"
  },
  "babel": {
    "presets": ["env"]
  }
}
